+using UnityEngine;
+using UnityEngine.SceneManagement; // Sahne bilgisini okumak iin gerekli
+
+public class CharacterScaler : MonoBehaviour
+{
+    void OnEnable()
+    {
+        // Sahne yklendiinde tetiklenecek olay dinlemeye bala
+        SceneManager.sceneLoaded += OnSceneLoaded;
+    }
+
+    void OnDisable()
+    {
+        // Dinlemeyi brak (Hata almamak iin nemli)
+        SceneManager.sceneLoaded -= OnSceneLoaded;
+    }
+
+    // Bu fonksiyon her sahne aldnda otomatik alr
+    void OnSceneLoaded(Scene scene, LoadSceneMode mode)
+    {
+        int sahneNo = scene.buildIndex;
+
+        // Sahne Numaralarna gre boyut ayarlamas
+        // Not: Build Profiles listesindeki numaralar kullanyoruz.
+        
+        if (sahneNo == 1) // LWL1 (Balang)
+        {
+            transform.localScale = new Vector3(1, 1, 1);
+        }
+        else if (sahneNo == 2) // LWL2 (kinci Sahne)
+        {
+            transform.localScale = new Vector3(3, 3, 1);
+        }
+        else if (sahneNo == 3) // LWL3 (nc Sahne)
+        {
+            transform.localScale = new Vector3(6, 6, 1);
+        }
+    }
+    
+    // Eer karakterin "DontDestroyOnLoad" deilse ve her sahnede yeniden oluuyorsa,
+    // Start fonksiyonu da ii garantiye almak iin kullanlabilir:
+    void Start()
+    {
+        OnSceneLoaded(SceneManager.GetActiveScene(), LoadSceneMode.Single);
+    }
+}
