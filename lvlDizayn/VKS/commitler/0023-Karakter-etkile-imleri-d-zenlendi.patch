+using UnityEngine;
+using UnityEngine.SceneManagement;
+
+public class GameOverManager : MonoBehaviour
+{
+    [Header("UI Paneli")]
+    public GameObject gameOverPanel; // Unity'de oluturacan siyah panel
+
+    public void ShowGameOver()
+    {
+        if (gameOverPanel != null)
+        {
+            gameOverPanel.SetActive(true); // Paneli a
+            Cursor.visible = true; // Mouse'u grnr yap
+            Cursor.lockState = CursorLockMode.None; // Mouse kilidini kaldr
+            
+            // Oyunu durdurmak istersen (Arkadaki dmanlar dursun):
+            Time.timeScale = 0f; 
+        }
+    }
+
+    // "Yeniden Dene" Butonu in
+    public void RetryGame()
+    {
+        Time.timeScale = 1f; // Zaman tekrar balat
+
+        // GameManager varsa cezal sistemi altr
+        if (GameManager.Instance != null)
+        {
+            GameManager.Instance.OyuncuOldu();
+        }
+        else
+        {
+            // Yoksa sahneyi dz yeniden balat
+            SceneManager.LoadScene(SceneManager.GetActiveScene().buildIndex);
+        }
+    }
+
+    // "Ana Men" Butonu in
+    public void LoadMainMenu()
+    {
+        Time.timeScale = 1f;
+        SceneManager.LoadScene(0); // Build Settings'deki ilk sahneye (Men) dn
+    }
+}
