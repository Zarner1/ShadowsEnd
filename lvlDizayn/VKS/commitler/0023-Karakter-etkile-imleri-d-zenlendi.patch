+using UnityEngine;
+using UnityEngine.SceneManagement;
+
+public class GameManager : MonoBehaviour
+{
+    public static GameManager Instance; // Her yerden erimek iin Singleton yaps
+
+    [Header("Ceza Ayarlar")]
+    public float baslangicMaxCan = 100f; // Oyunun en bandaki saf can
+    public float canAzalmaMiktari = 20f; // Her lmde silinecek max can miktar
+    public float minCanLimiti = 20f;     // Can buna derse oyun tamamen biter (Game Over)
+
+    private void Awake()
+    {
+        // Bu objenin sahneler aras yok olmasn engelle
+        if (Instance == null)
+        {
+            Instance = this;
+            DontDestroyOnLoad(gameObject);
+        }
+        else
+        {
+            Destroy(gameObject);
+        }
+    }
+
+    // Oyuncunun o anki Max Can deerini getirir
+    public float GetKayitliMaxCan()
+    {
+        // Eer kayt yoksa varsaylan deeri dndr
+        return PlayerPrefs.GetFloat("MaxHP", baslangicMaxCan);
+    }
+
+    // Oyuncu ldnde arlacak fonksiyon
+    public void OyuncuOldu()
+    {
+        float suankiMaxCan = GetKayitliMaxCan();
+        float yeniMaxCan = suankiMaxCan - canAzalmaMiktari;
+
+        if (yeniMaxCan < minCanLimiti)
+        {
+            Debug.Log("OYUN TAMAMEN BTT! HER EY SIFIRLANIYOR.");
+            TamamenSifirla();
+            SceneManager.LoadScene(0); // Ana menye dn
+        }
+        else
+        {
+            Debug.Log("ldn! Max cann dt: " + yeniMaxCan);
+            
+            // Yeni cezal can kaydet
+            PlayerPrefs.SetFloat("MaxHP", yeniMaxCan);
+            PlayerPrefs.Save();
+
+            // En batan balat (Sahne 1)
+            // Not: lerlemeyi (Unlocklar, itemler) korumak istiyorsan onlar silme, sadece sahneyi ykle.
+            SceneManager.LoadScene("LWL1"); 
+        }
+    }
+
+    // Oyunu tamamen sfrlamak iin (Mende 'New Game' butonu iin de kullanabilirsin)
+    public void TamamenSifirla()
+    {
+        PlayerPrefs.DeleteKey("MaxHP");
+        // Varsa dier kaytlar da (Itemler, Level kilidi) burada silebilirsin.
+    }
+}
