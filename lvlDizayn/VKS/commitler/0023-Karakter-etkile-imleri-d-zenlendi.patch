+using UnityEngine;
+using UnityEngine.SceneManagement;
+using TMPro; // TextMeshPro kullanyorsan bu ktphane gerekli (Yoksa UnityEngine.UI yeterli)
+using UnityEngine.UI; // Normal Text kullanyorsan bu gerekli
+
+public class DoorTrigger : MonoBehaviour
+{
+    [Header("Ayarlar")]
+    public string gidilecekSahneAdi = "LWL2";
+    public string dusmanTagi = "Enemy"; // Dmanlara verdiin Tag ismi
+    
+    [Header("UI Ayarlar")]
+    public GameObject bilgilendirmeObjesi; // Text objesinin kendisi (Ap kapatmak iin)
+    public Text bilgilendirmeYazisi;       // Yazy deitirmek iin (Legacy Text ise)
+    // public TMP_Text bilgilendirmeYazisi; // Eer TextMeshPro kullanyorsan sttekini sil bunu a
+
+    private bool oyuncuKapidaMi = false;
+
+    void Start()
+    {
+        if (bilgilendirmeObjesi != null)
+            bilgilendirmeObjesi.SetActive(false);
+    }
+
+    void Update()
+    {
+        // Kapdaysa ve E'ye bastysa
+        if (oyuncuKapidaMi && Input.GetKeyDown(KeyCode.E))
+        {
+            DusmanKontroluVeGecis();
+        }
+    }
+
+    void DusmanKontroluVeGecis()
+    {
+        // Sahnede bu Tag'e sahip ka obje kaldn sayar
+        int kalanDusman = GameObject.FindGameObjectsWithTag(dusmanTagi).Length;
+
+        if (kalanDusman <= 0)
+        {
+            // Hi dman kalmad, gei serbest!
+            SceneManager.LoadScene(gidilecekSahneAdi);
+        }
+        else
+        {
+            // Dmanlar hala hayatta!
+            Debug.Log("Hala " + kalanDusman + " dman var!");
+            
+            // Oyuncuya uyar verelim
+            if (bilgilendirmeYazisi != null)
+            {
+                bilgilendirmeYazisi.text = "nce tm dmanlar temizle! (" + kalanDusman + " kald)";
+            }
+        }
+    }
+
+    private void OnTriggerEnter2D(Collider2D other)
+    {
+        if (other.CompareTag("Player"))
+        {
+            oyuncuKapidaMi = true;
+            
+            if (bilgilendirmeObjesi != null)
+            {
+                bilgilendirmeObjesi.SetActive(true);
+                // Kapya ilk geldiinde standart yazy gsterelim
+                bilgilendirmeYazisi.text = "Sonraki seviye iin E tuuna bas"; 
+            }
+        }
+    }
+
+    private void OnTriggerExit2D(Collider2D other)
+    {
+        if (other.CompareTag("Player"))
+        {
+            oyuncuKapidaMi = false;
+            
+            if (bilgilendirmeObjesi != null)
+                bilgilendirmeObjesi.SetActive(false);
+        }
+    }
+}
