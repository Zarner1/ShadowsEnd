+using UnityEngine;
+
+public class CameraFollow : MonoBehaviour
+{
+    // Kamerann takip edecei karakter objesini Unity Inspector'dan atayn.
+    public Transform target; 
+    
+    [Header("Ayarlar")]
+    // Kamerann hedefe ne kadar yumuak (gecikmeli) ulaacan belirler. (rn: 0.125f)
+    public float smoothSpeed = 0.125f; 
+
+    // Y ekseninde takibi ap kapatma (Kapal tutarsanz kamera Y'de sabit kalr)
+    public bool followY = true; 
+    
+    [Header("Harita Snrlar (Clamping)")]
+    // Kamerann gidebilecei en sol (minimum) X pozisyonu
+    public float minXPosition = 0f; 
+    // Kamerann gidebilecei en sa (maksimum) X pozisyonu
+    public float maxXPosition = 50f; // Bu deeri haritanza gre ayarlayn
+    
+    // Kamerann gidebilecei en alt (minimum) Y pozisyonu
+    public float minYPosition = 0f; 
+    // Kamerann gidebilecei en st (maksimum) Y pozisyonu
+    public float maxYPosition = 10f; // Bu deeri haritanza gre ayarlayn
+
+
+    // Bu metot, karakter hareket ettikten sonra (her kare sonunda) arlr.
+    void LateUpdate()
+    {
+        // Hedef atanmadysa veya yok edildiyse, hata vermemek iin dur
+        if (target == null)
+            return;
+
+        // 1. HEDEFLENEN POZSYONU HESAPLA
+        
+        // Target X pozisyonunu al.
+        float targetX = target.position.x;
+        
+        // Eer followY true ise Target Y pozisyonunu al, deilse kamerann mevcut Y pozisyonunu (sabit kalr) al.
+        float targetY = followY ? target.position.y : transform.position.y;
+        
+        // Z eksenini (derinlik) sabit tut.
+        Vector3 targetPosition = new Vector3(targetX, targetY, transform.position.z);
+
+        // 2. YUMUAK GE HESAPLA
+        // Mevcut pozisyon ile hedef pozisyon arasnda yumuak bir gei yapar.
+        Vector3 smoothedPosition = Vector3.Lerp(transform.position, targetPosition, smoothSpeed);
+        
+        // 3. X VE Y POZSYONLARINI SINIRLA (CLAMP)
+        
+        // X pozisyonunu minXPosition ve maxXPosition arasnda kalacak ekilde snrla.
+        float clampedX = Mathf.Clamp(smoothedPosition.x, minXPosition, maxXPosition);
+
+        // Y pozisyonunu minYPosition ve maxYPosition arasnda kalacak ekilde snrla.
+        float clampedY = Mathf.Clamp(smoothedPosition.y, minYPosition, maxYPosition);
+
+        // 4. KAMERANIN YEN POZSYONUNU AYARLA
+        transform.position = new Vector3(clampedX, clampedY, transform.position.z);
+    }
 }
