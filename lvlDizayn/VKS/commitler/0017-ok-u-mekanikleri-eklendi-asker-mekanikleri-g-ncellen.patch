--- /dev/null
+++ b/Assets/enemy_archer_movement.cs
@@ -0,0 +1,182 @@
+using System.Collections;
+using System.Collections.Generic;
+using UnityEngine;
+
+public class Enemy_archer_movement : MonoBehaviour
+{
+    [Header("Hedef Ayarlar")]
+    public Transform player; 
+
+    [Header("Hareket Ayarlar")]
+    public float moveSpeed = 3f; 
+    public float stopDistance = 5f; 
+    public float meleeDistance = 1.5f; 
+
+    [Header("Saldr Ayarlar")]
+    public GameObject arrowPrefab; // Unity'de oluturduun ok prefab
+    public Transform firePoint;    // Okun kaca nokta
+    public float fireRate = 1.5f;  // At hz
+    private float nextFireTime = 0f;
+
+    [Header("Ok Fizik Ayarlar (YEN)")]
+    public float launchForce = 15f;  // Frlatma gc
+    public float arcModifier = 0.5f; // Havaya dikme oran (Kavis miktar)
+
+    [Header("Bileenler")]
+    private Animator anim;
+    private SpriteRenderer spriteRenderer;
+    private Rigidbody2D rb;      
+    private Collider2D col;      
+
+    // --- lm Kontrol ---
+    private bool isDead = false;
+
+    void Start()
+    {
+        anim = GetComponent<Animator>();
+        spriteRenderer = GetComponent<SpriteRenderer>();
+        rb = GetComponent<Rigidbody2D>();
+        col = GetComponent<Collider2D>();
+
+        if (player == null)
+        {
+            GameObject playerObj = GameObject.FindGameObjectWithTag("Player");
+            if (playerObj != null)
+            {
+                player = playerObj.transform;
+            }
+        }
+    }
+
+    void Update()
+    {
+        if (isDead) return;
+        if (player == null) return;
+
+        float distanceToPlayer = Vector2.Distance(transform.position, player.position);
+
+        FlipTowardsPlayer();
+
+        // 1. DURUM: Karakter ok uzakta -> KOVALA
