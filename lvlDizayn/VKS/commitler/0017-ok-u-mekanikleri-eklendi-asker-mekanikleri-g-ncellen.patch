+            GameObject newArrow = Instantiate(arrowPrefab, firePoint.position, Quaternion.identity);
+            
+            // 2. Okun Rigidbody'sine ula
+            Rigidbody2D arrowRb = newArrow.GetComponent<Rigidbody2D>();
