+using UnityEngine;
+
+public class CameraFollow : MonoBehaviour
+{
+    // Kamerann takip edecei karakter objesi
+    public Transform target; 
+
+    // Kamerann sadece X ekseninde mi hareket etmesini istiyorsunuz?
+    public bool followX = true; 
+    
+    // Kamerann sadece Y ekseninde mi hareket etmesini istiyorsunuz?
+    public bool followY = true; 
+    
+    // Kamerann hedefe ne kadar yumuak (gecikmeli) ulaacan belirler. 
+    // Daha kk deerler daha hzl takip demektir. (rn: 0.1)
+    public float smoothSpeed = 0.125f; 
+
+    // Unity bu metodu her kare sonunda arr. 
+    // Kameray hareket ettirmek iin idealdir, bylece karakterin hareketi tamamlanr.
+    void LateUpdate()
+    {
+        // Eer takip edilecek bir hedef atanmamsa, hibir ey yapma.
+        if (target == null)
+            return;
+
+        // Hedef pozisyonunu al. Kamerann Z pozisyonunu koru.
+        float targetX = followX ? target.position.x : transform.position.x;
+        float targetY = followY ? target.position.y : transform.position.y;
+        
+        Vector3 targetPosition = new Vector3(targetX, targetY, transform.position.z);
+
+        // Kamerann mevcut pozisyonu ile hedef pozisyonu arasnda yumuak bir gei yap.
+        Vector3 smoothedPosition = Vector3.Lerp(transform.position, targetPosition, smoothSpeed);
+        
+        // Kamerann yeni pozisyonunu ayarla.
+        transform.position = smoothedPosition;
+    }
+}
